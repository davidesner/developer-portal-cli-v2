{
    "name": "keboola/developer-portal-cli-v2",
    "description": "Command line client for Keboola Developer Portal",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ondrej Hlavacek",
            "email": "ondra@keboola.com"
        }
    ],
    "require": {
        "symfony/console": "^3.2",
        "keboola/developer-portal-php-client": "^3.0",
        "symfony/process": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Keboola\\DeveloperPortal\\": "src/Keboola/DeveloperPortal" }
    },
    "autoload-dev": {
        "psr-4": { "Keboola\\DeveloperPortal\\Test\\": "tests/Keboola/DeveloperPortal" }
    }
}
